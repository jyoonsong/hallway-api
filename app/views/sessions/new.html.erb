<% if logged_in? %>
    You came to wrong page
<% else %>
    <%= form_for @user, url: sessions_path, method: :post do |f| %>
        <%= f.label :email %>
        <%= f.text_field :email %>

        <%= f.label :password %>
        <%= f.password_field :password %>

        <%= f.submit "log in" %>
    <% end %>
<% end %>